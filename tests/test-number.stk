(test-subsection "complex writer syntax")

(test "complex printing"
      '("+inf.0+inf.0i" "+nan.0+inf.0i" "+inf.0+inf.0i"
       "-inf.0-inf.0i"  "+nan.0+nan.0i" "+nan.0+nan.0i"
       "1+1i"           "0+1i"          "1+0.0i"
       "1-0.0i"         "1+1/2i"        "2-1/3i")
